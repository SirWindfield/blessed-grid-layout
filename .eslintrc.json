{
	"parser": "babel-eslint",
	"extends": ["plugin:flowtype/recommended", "xo", "prettier"],
	"env": {
		"jest": true,
		"node": true
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"printWidth": 90,
				"useTabs": true
			}
		]
	},
	"plugins": ["flowtype", "prettier"]
}
